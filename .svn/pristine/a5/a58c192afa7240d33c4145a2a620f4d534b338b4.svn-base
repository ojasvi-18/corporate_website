.portfolio-items .portfolio-item:before { content: ""; border: 2px solid #00a0de; bottom: 1.25rem; left: 1.25rem; position: absolute; right: 1.25rem; top: 1.25rem; z-index: 2; opacity: 0; transform: scale(1.15); transition: 0.4s; }
.portfolio-items .portfolio-item:after { content: ""; background: rgba(0, 0, 0, 0.7); position: absolute; top: 0; left: 0; height: 100%; width: 100%; opacity: 0; transition: 0.4s; }
.portfolio-items .portfolio-item a { color: white; }
