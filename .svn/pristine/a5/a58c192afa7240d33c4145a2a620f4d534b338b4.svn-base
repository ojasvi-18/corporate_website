.tipr_content { padding: 0.35rem 1rem 0.4rem; background: rgba(0, 0, 0, 0.65); border-radius: 0.4rem; color: #ffffff; font-size: 1rem; }

.tipr_container_bottom, .tipr_container_top { display: none; position: absolute; max-width: 300px; z-index: 1000; }

.tipr_container_bottom { margin-top: 13px; }

.tipr_container_top { margin-top: -50px; }

.tipr_point_top, .tipr_point_bottom { position: relative; background: rgba(0, 0, 0, 0.65); border-radius: 0.4rem; width: auto; }

