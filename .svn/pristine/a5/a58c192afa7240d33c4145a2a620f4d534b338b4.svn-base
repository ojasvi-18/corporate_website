body .cp-nav-container > ul > li > ul { transition: 0.4s; position: absolute; top: 0; left: 14rem; background: #1C1C1C; border-radius: 0.4rem; font-size: 0.9em; width: 88%; height: 100%; padding: 0.5rem 2rem; margin: 0; overflow: auto; }
body .cp-nav-container > ul > li > ul li a { color: white; }
